		"tab.activeBorder": "#0df2d7",
		"tab.inactiveBackground": "#0f2033",
		"tab.border": "#0f2033",
		"tab.inactiveForeground": "#495869",
		"tab.unfocusedActiveBackground": "#0f2033",
		"tab.unfocusedActiveBorder": "#0df2d7",
		"tab.unfocusedActiveForeground": "#495869",
		"panelTitle.activeBorder": "#0df2d7",
		"panel.border": "#243242",
